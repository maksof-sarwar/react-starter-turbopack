{
	"version": 2,
	"cleanUrls": true,
	"builds": [
		{
			"src": "build/server/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "build/web/index.js",
			"use": "@vercel/node"
		}
	],
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "build/server/index.js"
		},
		{
			"source": "/(.*)",
			"destination": "build/web/index.js"
		}
	]
}
