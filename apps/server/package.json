{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"npm": "'use yarn'",
		"yarn": ">= 1.22.19",
		"pnpm": ">= 7.25.1"
	},
	"scripts": {
		"build": "tsup src/index.ts --format cjs",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
		"lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
		"db": "prisma db push",
		"schemix": "ts-node-dev ./prisma/base.ts && prisma format",
		"studio": "prisma studio",
		"postinstall": "prisma generate",
		"dbsync": "pnpm run schemix && pnpm run db && pnpm run postinstall"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^4.9.0",
		"@trpc/server": "^10.9.0",
		"express": "^4.18.2",
		"fastify": "^4.12.0",
		"fastify-healthcheck": "^4.4.0"
	},
	"devDependencies": {
		"@fastify/cookie": "^8.3.0",
		"@fastify/cors": "^8.2.0",
		"@types/express": "^4.17.16",
		"@types/node": "*",
		"pino": "^8.8.0",
		"pino-pretty": "^9.1.1",
		"prisma": "^4.9.0",
		"rimraf": "^4.1.1",
		"schemix": "^1.8.5",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"zod": "^3.20.2"
	}
}
