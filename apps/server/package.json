{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"npm": "'use yarn'",
		"yarn": ">= 1.22.19",
		"pnpm": ">= 7.25.1"
	},
	"scripts": {
		"build": "pnpm clean && tsup index.ts --format cjs",
		"clean": " rimraf dist",
		"dev": "ts-node-dev -r tsconfig-paths/register ./index.ts",
		"lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
		"db": "prisma db push",
		"schemix": "ts-node-dev ./prisma/base.ts && prisma format",
		"studio": "prisma studio",
		"postinstall": "pnpm run dbsync",
		"dbsync": "pnpm run schemix && pnpm run db && prisma generate"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "4.9.0",
		"@trpc/server": "^10.9.0",
		"fastify": "^4.12.0"
	},
	"devDependencies": {
		"@fastify/cookie": "^8.3.0",
		"@fastify/cors": "^8.2.0",
		"@fastify/jwt": "^6.5.0",
		"@fastify/sensible": "^5.2.0",
		"@fastify/session": "^10.1.1",
		"@fastify/static": "^6.8.0",
		"@fastify/swagger": "^8.3.1",
		"@types/node": "*",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"fastify-healthcheck": "^4.4.0",
		"fastify-zod": "^1.2.0",
		"pino": "^8.8.0",
		"pino-pretty": "^9.1.1",
		"prisma": "^4.9.0",
		"rimraf": "^4.1.1",
		"schemix": "^1.8.5",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.2",
		"tsc-silent": "^1.2.2",
		"tsconfig": "workspace:*",
		"tsconfig-paths": "^4.1.2",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"zod": "^3.20.2"
	}
}
